    position: relative;
    background: #fff;
}
.welcome-section{
    background:#d9f3fa;
    background-image: url('~images/bg/hero_wide.jpg');
    background-position: top left;
    background-size: cover;
    clip-path: polygon(0% 0%, 0% 93%, 50% 100%, 100% 93%, 100% 0%);
}

